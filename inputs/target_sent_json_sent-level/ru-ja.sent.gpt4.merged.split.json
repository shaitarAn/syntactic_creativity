{"translation": "\u300c\u5618\u3092\u3064\u3044\u3066\u3044\u308b\u300d\u3068\u79c1\u306f\u601d\u3063\u305f\u3002"}
{"translation": "\u30ad\u30fc\u30db\u30eb\u30c0\u30fc\u3068\u5c0f\u6307\u306e\u6307\u8f2a\u306f\u3001\u30d1\u30f3\u306e\u4e00\u5207\u308c\u306e\u305f\u3081\u306b\u624b\u7d19\u3067\u7d50\u3073\u3064\u304f\u3053\u3068\u304c\u96e3\u3057\u304b\u3063\u305f\u3057\u3001\u306a\u304a\u304b\u3064\u30ab\u30e0\u30b7\u30a7\u30d5\u306e\u9854\u306b\u306f\u3001\u7d4c\u9a13\u8c4a\u5bcc\u306a\u76ee\u3067\u6349\u3048\u3089\u308c\u308b\u308f\u305a\u304b\u306a\u96f2\u304c\u6d41\u308c\u3001\u305d\u308c\u306f\u6ec5\u591a\u306b\u5618\u3092\u3064\u304f\u4eba\u3005\u306e\u9854\u306b\u3057\u304b\u898b\u3089\u308c\u307e\u305b\u3093\u3002"}
{"translation": "\u3042\u306a\u305f\u306e\u5c0f\u8aac\u306e\u7b4b\u66f8\u304d\u306f\u4f55\u3067\u3059\u304b\uff1f\u3068\u79c1\u306f\u5c0b\u306d\u307e\u3057\u305f\u3002"}
{"translation": "\u30b9\u30c8\u30fc\u30ea\u30fc\u3002"}
{"translation": "\u3069\u3046\u8a00\u3063\u3066\u3044\u3044\u304b\u306a\uff1f \u30b9\u30c8\u30fc\u30ea\u30fc\u306f\u65b0\u3057\u304f\u3042\u308a\u307e\u305b\u3093\u3002"}
{"translation": "\u611b\u3001\u6bba\u4eba\u3002"}
{"translation": "\u306f\u3044\u3001\u3042\u306a\u305f\u304c\u8aad\u3093\u3067\u3001\u898b\u308b\u3067\u3057\u3087\u3046\u3002"}
{"translation": "\u300c\u6cd5\u5ef7\u635c\u67fb\u5b98\u306e\u30e1\u30e2\u304b\u3089\u300d"}
{"translation": "\u30ab\u30df\u30b7\u30a7\u30d5\u306f\u79c1\u306b\u305d\u306e\u512a\u3057\u3044\u76ee\u3067\u898b\u4e0a\u3052\u3001\u9759\u304b\u306b\u7b11\u3063\u305f\u3002"}
{"translation": "\u3046\u3093\u3056\u308a\u3059\u308b\u3088\u3046\u306a\u4ed5\u4e8b\u3002"}
{"translation": "\u4ed5\u3048\u3066\u3044\u305f\u3001\u624b\u3092\u632f\u3063\u305f\u305d\u3057\u3066\u3084\u3081\u305f\u3002"}
{"translation": "\u79c1\u306b\u306f\u3082\u3046\u6388\u696d\u306f\u306a\u304f\u3001\u307b\u3068\u3093\u3069\u4f55\u3082\u98df\u3079\u308b\u3082\u306e\u304c\u3042\u308a\u307e\u305b\u3093\u3002"}
{"translation": "\u305d\u3057\u3066\u3001\u79c1\u306e\u7269\u8a9e\u306e\u4fa1\u5024\u3092\u7121\u8996\u3057\u3066\u5370\u5237\u3059\u308b\u306a\u3089\u3001\u305d\u308c\u306f\u79c1\u306b\u3068\u3063\u3066\u5358\u306a\u308b\u597d\u610f\u4ee5\u4e0a\u306e\u3053\u3068\u306b\u306a\u308a\u307e\u3059\u3002"}
{"translation": "\u3042\u306a\u305f\u306f\u79c1\u3092\u52a9\u3051\u3066\u304f\u308c\u307e\u3059\u3002"}
{"translation": "\u65b0\u805e\u306f\u6148\u5584\u9662\u3067\u3082\u3001\u7570\u56fd\u4eba\u306e\u305f\u3081\u306e\u53d7\u3051\u5165\u308c\u30cf\u30a6\u30b9\u3067\u3082\u3042\u308a\u307e\u305b\u3093\u3002"}
{"translation": "\u79c1\u306f\u305d\u308c\u3092\u77e5\u3063\u3066\u3044\u307e\u3059\u304c\u3001\u3057\u304b\u3057\u3002"}
{"translation": "\u3042\u306a\u305f\u304c\u89aa\u5207\u3067\u3042\u3063\u3066\u304f\u3060\u3055\u3044\u3002"}
{"translation": "\u3068\u3066\u3082\u5b09\u3057\u3044\u3067\u3059\u3002"}
{"translation": "\u4f55\u304b\u304a\u624b\u4f1d\u3044\u3067\u304d\u308b\u3053\u3068\u306f\u3042\u308a\u307e\u3059\u304b\uff1f"}
{"translation": "\u59cb\u3081\u308b\u3053\u3068\u3092\u671b\u3093\u3067\u3044\u308b\u8005\u304c\u5ea7\u308a\u3001\u5f7c\u306e\u8a34\u3048\u308b\u3088\u3046\u306a\u76ee\u3067\u5e8a\u3092\u898b\u3064\u3081\u7d9a\u3051\u307e\u3057\u305f\u3002"}
{"translation": "\u79c1\u306f\u3042\u306a\u305f\u306e\u65b0\u805e\u306b\u63b2\u8f09\u3057\u305f\u3044\u5c0f\u3055\u306a\u5c0f\u8aac\u3092\u6301\u3063\u3066\u304d\u307e\u3057\u305f\u3002"}
{"translation": "\u79c1\u306f\u3042\u306a\u305f\u306b\u6b63\u76f4\u306b\u8a00\u3044\u307e\u3059\u3001\u7de8\u96c6\u8005\u6bbf\uff1a\u79c1\u304c\u81ea\u5206\u306e\u8a71\u3092\u66f8\u3044\u305f\u7406\u7531\u306f\u3001\u4f5c\u5bb6\u306e\u540d\u58f0\u3084\u7518\u7f8e\u306a\u8cdb\u7f8e\u306e\u305f\u3081\u3067\u306f\u3042\u308a\u307e\u305b\u3093\u3002"}
{"translation": "\u3053\u308c\u3089\u306e\u826f\u3044\u7269\u306b\u5bfe\u3057\u3066\u3001\u79c1\u306f\u3059\u3067\u306b\u9ad8\u9f62\u3067\u3059\u3002"}
{"translation": "\u3060\u304b\u3089\u3001\u79c1\u306f\u7d14\u7c8b\u306b\u5546\u696d\u7684\u306a\u52d5\u6a5f\u304b\u3089\u4f5c\u5bb6\u306e\u9053\u306b\u9032\u3080\u3002"}
{"translation": "\u7a3c\u304e\u305f\u3044\u3067\u3059\u3002"}
{"translation": "\u79c1\u306f\u4eca\u3001\u6c7a\u3057\u3066\u4f55\u306e\u4ed5\u4e8b\u3082\u6301\u3063\u3066\u3044\u307e\u305b\u3093\u3002"}
{"translation": "\u3042\u306a\u305f\u304c\u77e5\u3063\u3066\u3044\u308b\u3088\u3046\u306b\u3001S\u5730\u533a\u306e\u5224\u4e8b\u88dc\u3067\u30015\u5e74\u4ee5\u4e0a\u52e4\u3081\u3066\u3044\u307e\u3057\u305f\u304c\u3001\u8ca1\u7523\u3092\u7bc9\u304f\u3053\u3068\u3082\u7121\u5b9f\u3092\u4fdd\u3064\u3053\u3068\u3082\u3042\u308a\u307e\u305b\u3093\u3067\u3057\u305f\u3002"}
{"translation": "\u300c\u3057\u304b\u3057\u3001\u5f7c\u306f\u672c\u5f53\u306b\u591a\u304f\u8a71\u3059\u306d\uff01\u300d\u3068\u79c1\u306f\u601d\u3044\u3001\u30da\u30f3\u306b\u624b\u3092\u4f38\u3070\u3057\u3066\u3001\u81ea\u5206\u306b\u4f59\u88d5\u304c\u306a\u3044\u3053\u3068\u3092\u793a\u3057\u307e\u3057\u305f\u3002"}
{"translation": "\uff08\u3042\u307e\u308a\u306b\u3082\u305d\u306e\u6642\u306e\u8a2a\u554f\u8005\u306b\u306f\u3046\u3093\u3056\u308a\u3057\u3066\u3044\u305f\uff01\uff09 \u79c1\u306f\u3042\u306a\u305f\u304b\u3089\u305f\u3063\u305f\u4e00\u5206\u3060\u3051\u9802\u304d\u307e\u3059\uff01\u3068\u79c1\u306e\u30d2\u30fc\u30ed\u30fc\u306f\u8b1d\u7f6a\u3059\u308b\u3088\u3046\u306a\u58f0\u3067\u7d9a\u3051\u307e\u3057\u305f\u3002"}
{"translation": "\u3057\u304b\u3057\u3001\u307e\u305a\u7b2c\u4e00\u306b\u81ea\u5df1\u7d39\u4ecb\u3055\u305b\u3066\u304f\u3060\u3055\u3044\u3002"}
{"translation": "\u6cd5\u5b66\u306e\u5019\u88dc\u8005\u3067\u3042\u308b\u30a4\u30f4\u30a1\u30f3\u30fb\u30da\u30c8\u30ed\u30f4\u30a3\u30c1\u30fb\u30ab\u30de\u30b7\u30a7\u30d5\u3001\u5143\u5211\u4e8b\u635c\u67fb\u5b98\u3002"}
{"translation": "\u7b46\u3092\u57f7\u308b\u8005\u305f\u3061\u306b\u5c5e\u3059\u308b\u3053\u3068\u306f\u306a\u3044\u306e\u3067\u3059\u304c\u3001\u305d\u308c\u3067\u3082\u306a\u304a\u3001\u7d14\u7c8b\u306b\u4f5c\u5bb6\u3068\u3057\u3066\u306e\u76ee\u7684\u3067\u53c2\u308a\u307e\u3057\u305f\u3002"}
{"translation": "\u3042\u306a\u305f\u306e\u524d\u306b\u306f\u300140\u6b73\u8fd1\u304f\u306b\u3082\u304b\u304b\u308f\u3089\u305a\u521d\u5fc3\u8005\u30af\u30e9\u30b9\u306b\u53c2\u52a0\u3057\u305f\u3044\u3068\u9858\u3063\u3066\u3044\u308b\u4eba\u304c\u3044\u307e\u3059\u3002"}
{"translation": "\u3067\u3082\u3001\u9045\u3044\u3088\u308a\u5168\u304f\u306a\u3044\u65b9\u304c\u307e\u3057\u3067\u3059\u3002"}
{"translation": "\u5f7c\u304c\u79c1\u306e\u30aa\u30d5\u30a3\u30b9\u306b\u5165\u3063\u3066\u304d\u305f\u3068\u304d\u3001\u5f7c\u306f\u56f0\u60d1\u3057\u307e\u3057\u305f\u3002"}
{"translation": "\u5f7c\u306e\u512a\u3057\u304f\u3001\u654f\u611f\u306a\u6027\u683c\u306f\u3001\u304a\u305d\u3089\u304f\u79c1\u306e\u3057\u304b\u3081\u3063\u9762\u3001\u4e0d\u6a5f\u5acc\u306a\u8868\u60c5\u306b\u30b7\u30e7\u30c3\u30af\u3092\u53d7\u3051\u305f\u3067\u3057\u3087\u3046\u3002"}
{"translation": "\u304a\u9858\u3044\u3057\u307e\u3059\u3001\u795e\u306e\u305f\u3081\u306b\uff01 \u5f7c\u306f\u3084\u308f\u3089\u304b\u304f\u3001\u8c4a\u304b\u306a\u30d0\u30ea\u30c8\u30f3\u3067\u59cb\u3081\u305f\u3002"}
{"translation": "\u79c1\u306f\u3042\u306a\u305f\u306e\u5fd9\u3057\u3044\u6642\u9593\u306b\u62bc\u3057\u639b\u3051\u3066\u3001\u81ea\u5206\u306e\u305f\u3081\u306b\u7279\u5225\u6271\u3044\u3092\u3057\u3066\u3082\u3089\u3046\u3088\u3046\u306b\u5f37\u5236\u3057\u307e\u3059\u3002"}
{"translation": "\u3042\u306a\u305f\u306f\u3068\u3066\u3082\u5fd9\u3057\u3044\u3067\u3059\u306d\uff01"}
{"translation": "\u3067\u3082\u3001\u554f\u984c\u306f\u3053\u3053\u306b\u3042\u308b\u3093\u3067\u3059\u3001\u7de8\u96c6\u9577\u3055\u3093\u3002"}
{"translation": "\u79c1\u306f\u660e\u65e5\u3001\u975e\u5e38\u306b\u91cd\u8981\u306a\u7528\u4ef6\u3067\u30aa\u30c7\u30c3\u30b5\u306b\u51fa\u767a\u3057\u307e\u3059\u3002"}
{"translation": "\u79c1\u304c\u3053\u306e\u65c5\u884c\u3092\u571f\u66dc\u65e5\u307e\u3067\u5ef6\u671f\u3059\u308b\u3053\u3068\u304c\u3067\u304d\u308c\u3070\u3001\u4fe1\u3058\u3066\u304f\u3060\u3055\u3044\u3001\u79c1\u306f\u3042\u306a\u305f\u306b\u7279\u4f8b\u3092\u4f5c\u3063\u3066\u3082\u3089\u3046\u3053\u3068\u3092\u983c\u3080\u3053\u3068\u306f\u3042\u308a\u307e\u305b\u3093\u3002"}
{"translation": "\u79c1\u306f\u898f\u5247\u3092\u5c0a\u91cd\u3057\u3066\u3044\u307e\u3059\u3001\u306a\u305c\u306a\u3089\u79e9\u5e8f\u3092\u611b\u3057\u3066\u3044\u308b\u304b\u3089\u3067\u3059\u3002"}
{"translation": "\u6817\u8272\u306e\u9aea\u3068\u3072\u3052\u306f\u3001\u30b7\u30eb\u30af\u306e\u3088\u3046\u306b\u8c4a\u304b\u3067\u67d4\u3089\u304b\u3044\u3002"}
{"translation": "\u67d4\u3089\u304b\u3044\u9aea\u306f\u3001\u67d4\u3089\u304b\u304f\u3001\u512a\u3057\u304f\u3001\u300c\u30b7\u30eb\u30af\u306e\u3088\u3046\u306a\u300d\u9b42\u306e\u8a3c\u3060\u3068\u8a00\u308f\u308c\u3066\u3044\u307e\u3059\u3002"}
{"translation": "\u72af\u7f6a\u8005\u3084\u60aa\u3044\u3001\u9811\u56fa\u306a\u6027\u683c\u306e\u4eba\u3005\u306f\u3001\u307b\u3068\u3093\u3069\u306e\u5834\u5408\u3001\u786c\u3044\u9aea\u3092\u6301\u3063\u3066\u3044\u307e\u3059\u3002"}
{"translation": "\u305d\u308c\u304c\u672c\u5f53\u304b\u3069\u3046\u304b\u3001\u8aad\u8005\u306f\u518d\u5ea6\u3001\u4ee5\u4e0b\u3067\u78ba\u8a8d\u3059\u308b\u3067\u3057\u3087\u3046\u3002"}
{"translation": "\u9854\u306e\u8868\u60c5\u3082\u3001\u3072\u3052\u3082\u3001\u30b3\u30ab\u30fc\u30c9\u4ed8\u304d\u306e\u7d33\u58eb\u306b\u3068\u3063\u3066\u5927\u304d\u304f\u3066\u91cd\u3044\u4f53\u306e\u52d5\u304d\u307b\u3069\u67d4\u3089\u304b\u304f\u3001\u512a\u3057\u304f\u306f\u3042\u308a\u307e\u305b\u3093\u3002"}
{"translation": "\u3053\u308c\u3089\u306e\u52d5\u304d\u306b\u306f\u3001\u793c\u5100\u6b63\u3057\u3055\u3001\u8efd\u3084\u304b\u3055\u3001\u512a\u96c5\u3055\u3001\u305d\u3057\u3066\u8868\u73fe\u3092\u304a\u8a31\u3057\u3044\u305f\u3060\u3051\u308c\u3070\u3042\u308b\u7a0b\u5ea6\u306e\u5973\u6027\u3089\u3057\u3055\u304c\u7686\u76ee\u898b\u3048\u307e\u3059\u3002"}
{"translation": "\u79c1\u306e\u82f1\u96c4\u306b\u306f\u3001\u8e44\u9244\u3092\u66f2\u3052\u305f\u308a\u3001\u30b5\u30fc\u30c7\u30a3\u30f3\u7f36\u3092\u63e1\u308a\u6f70\u3057\u305f\u308a\u3059\u308b\u306e\u306b\u591a\u304f\u306e\u52aa\u529b\u306f\u5fc5\u8981\u3042\u308a\u307e\u305b\u3093\u304c\u3001\u305d\u306e\u4e00\u65b9\u3067\u5f7c\u306e\u3069\u3093\u306a\u52d5\u304d\u3082\u5f7c\u304c\u7269\u7406\u7684\u306b\u5f37\u3044\u3053\u3068\u3092\u793a\u3057\u3066\u3044\u307e\u305b\u3093\u3002"}
{"translation": "\u5f7c\u306f\u30c9\u30a2\u30ce\u30d6\u3084\u5e3d\u5b50\u3092\u3001\u307e\u308b\u3067\u8776\u3092\u6271\u3046\u3088\u3046\u306b\uff1a\u512a\u3057\u304f\u3001\u614e\u91cd\u306b\u3001\u6307\u5148\u3067\u307b\u3093\u306e\u308a\u3068\u89e6\u308c\u3066\u53d6\u308a\u307e\u3059\u3002"}
{"translation": "\u5f7c\u306e\u8db3\u97f3\u306f\u9759\u304b\u3067\u3001\u63e1\u624b\u306f\u5f31\u3044\u3002"}
{"translation": "\u5f7c\u3092\u898b\u3066\u3044\u308b\u3068\u3001\u5f7c\u304c\u30b4\u30ea\u30a2\u30c6\u306e\u3088\u3046\u306b\u5f37\u5927\u3067\u3001\u4e00\u3064\u306e\u624b\u3067\u30a2\u30f3\u30c9\u30ec\u30a4\u7de8\u96c6\u8005\u305f\u30615\u4eba\u304c\u6301\u3061\u4e0a\u3052\u308b\u3053\u3068\u304c\u3067\u304d\u306a\u3044\u3082\u306e\u3092\u6301\u3061\u4e0a\u3052\u308b\u3053\u3068\u304c\u3067\u304d\u308b\u3068\u3044\u3046\u4e8b\u5b9f\u3092\u5fd8\u308c\u3066\u3057\u307e\u3044\u307e\u3059\u3002"}
{"translation": "\u5f7c\u306e\u8efd\u3084\u304b\u306a\u52d5\u304d\u3092\u898b\u3066\u3044\u308b\u3068\u3001\u5f7c\u304c\u5f37\u304f\u3066\u91cd\u3044\u3068\u306f\u4fe1\u3058\u3089\u308c\u307e\u305b\u3093\u3002"}
{"translation": "\u30b9\u30da\u30f3\u30b5\u30fc\u306f\u5f7c\u3092\u512a\u96c5\u3055\u306e\u6a21\u7bc4\u3068\u547c\u3076\u3053\u3068\u304c\u3067\u304d\u305f\u3067\u3057\u3087\u3046\u3002"}
{"translation": "\u305d\u306e\u9854\u5168\u4f53\u304b\u3089\u306f\u3001\u7d20\u6734\u3055\u3001\u5e83\u7bc4\u3067\u76f4\u622a\u306a\u6027\u8cea\u3001\u771f\u5b9f\u304c\u6ea2\u308c\u51fa\u3066\u3044\u308b\u3002"}
{"translation": "\u3082\u3057\u5618\u3067\u306f\u306a\u304f\u3001\u9854\u304c\u5fc3\u306e\u93e1\u3067\u3042\u308b\u3068\u3059\u308b\u306a\u3089\u3001\u521d\u65e5\u306b\u30b3\u30ab\u30fc\u30c9\uff08\u52f2\u7ae0\uff09\u3092\u6301\u3064\u7d33\u58eb\u3068\u51fa\u4f1a\u3063\u305f\u6642\u3001\u79c1\u306f\u5f7c\u304c\u5618\u3092\u3064\u304f\u3053\u3068\u304c\u3067\u304d\u306a\u3044\u3068\u6b63\u76f4\u306b\u8a00\u3046\u3053\u3068\u304c\u3067\u304d\u307e\u3059\u3002"}
{"translation": "\u79c1\u306f\u8ced\u3051\u3066\u3082\u3044\u3044\u304f\u3089\u3044\u3060\u3002"}
{"translation": "\u79c1\u304c\u8ced\u3051\u306b\u8ca0\u3051\u305f\u304b\u3069\u3046\u304b\u306f\u3001\u8aad\u8005\u304c\u3053\u308c\u304b\u3089\u898b\u308b\u3067\u3057\u3087\u3046\u3002"}
{"translation": "\u5f7c\u306f\u3001\u79c1\u304c\u3059\u3067\u306b\u8ff0\u3079\u305f\u3088\u3046\u306b\u3001\u80cc\u304c\u9ad8\u304f\u3066\u80a9\u5e45\u304c\u5e83\u304f\u3001\u9811\u4e08\u306a\u826f\u3044\u52b4\u50cd\u99ac\u306e\u3088\u3046\u3067\u3059\u3002"}
{"translation": "\u5f7c\u306e\u5168\u8eab\u304c\u5065\u5eb7\u3068\u529b\u3067\u6e80\u3061\u3066\u3044\u308b\u3002"}
{"translation": "\u9854\u306f\u30d4\u30f3\u30af\u8272\u3001\u624b\u306f\u5927\u304d\u304f\u3001\u80f8\u306f\u5e83\u304f\u7b4b\u8089\u8cea\u3067\u3001\u9aea\u306e\u6bdb\u306f\u5065\u5eb7\u7684\u306a\u7537\u306e\u5b50\u306e\u3088\u3046\u306b\u6fc3\u3044\u3067\u3059\u3002"}
{"translation": "\u5f7c\u306f40\u6b73\u524d\u5f8c\u3067\u3059\u3002"}
{"translation": "\u5f7c\u306f\u6700\u65b0\u306e\u6d41\u884c\u306b\u5408\u308f\u305b\u3066\u3001\u65b0\u3057\u304f\u6700\u8fd1\u7e2b\u88fd\u3055\u308c\u305f\u83ef\u9e97\u306a\u30b9\u30fc\u30c4\u3092\u7740\u7528\u3057\u3066\u3044\u307e\u3059\u3002"}
{"translation": "\u80f8\u306b\u306f\u5927\u304d\u306a\u91d1\u306e\u30c1\u30a7\u30fc\u30f3\u3068\u30c1\u30e3\u30fc\u30e0\u304c\u3001\u5c0f\u6307\u306b\u306f\u5c0f\u3055\u306a\u660e\u308b\u3044\u661f\u306e\u3088\u3046\u306b\u8f1d\u304f\u30c0\u30a4\u30e4\u30e2\u30f3\u30c9\u306e\u30ea\u30f3\u30b0\u304c\u3061\u3089\u3064\u3044\u3066\u3044\u307e\u3059\u3002"}
{"translation": "\u3057\u304b\u3057\u3001\u4f55\u3088\u308a\u3082\u91cd\u8981\u306a\u3053\u3068\u3001\u305d\u3057\u3066\u7269\u8a9e\u3084\u5c0f\u8aac\u306e\u3061\u3087\u3063\u3068\u3057\u305f\u7acb\u6d3e\u306a\u4e3b\u4eba\u516c\u306b\u3068\u3063\u3066\u306f\u975e\u5e38\u306b\u91cd\u8981\u306a\u3053\u3068\u306f\u3001\u5f7c\u304c\u975e\u5e38\u306b\u7f8e\u3057\u3044\u3068\u3044\u3046\u3053\u3068\u3067\u3059\u3002"}
{"translation": "\u79c1\u306f\u5973\u6027\u3067\u3082\u82b8\u8853\u5bb6\u3067\u3082\u3042\u308a\u307e\u305b\u3093\u3002"}
{"translation": "\u79c1\u306f\u7537\u6027\u306e\u7f8e\u3057\u3055\u306b\u3064\u3044\u3066\u3088\u304f\u7406\u89e3\u3057\u3066\u3044\u306a\u3044\u304c\u3001\u5f7c\u306e\u30b3\u30ab\u30fc\u30c7\u3092\u4ed8\u3051\u305f\u305d\u306e\u5916\u89b3\u306f\u79c1\u306b\u5370\u8c61\u3092\u4e0e\u3048\u305f\u3002"}
{"translation": "\u5f7c\u306e\u5927\u304d\u306a\u7b4b\u8089\u8cea\u306a\u9854\u306f\u79c1\u306e\u8a18\u61b6\u306b\u6c38\u9060\u306b\u6b8b\u308a\u307e\u3057\u305f\u3002"}
{"translation": "\u3053\u306e\u9854\u306b\u306f\u3001\u672c\u7269\u306e\u30ae\u30ea\u30b7\u30e3\u98a8\u306e\u9f3b\u3068\u305d\u306e\u30d2\u30c0\u3001\u7d30\u3044\u5507\u3001\u305d\u3057\u3066\u7f8e\u3057\u3044\u9752\u3044\u76ee\u304c\u3042\u308a\u307e\u3059\u3002"}
{"translation": "\u305d\u306e\u76ee\u306b\u306f\u512a\u3057\u3055\u3068\u540d\u524d\u3092\u3064\u3051\u308b\u306e\u304c\u96e3\u3057\u3044\u4f55\u304b\u304c\u8f1d\u3044\u3066\u3044\u307e\u3059\u3002"}
{"translation": "\u305d\u308c\u306f\u3001\u5c0f\u3055\u306a\u52d5\u7269\u304c\u60b2\u3057\u307f\u3084\u82e6\u75db\u3092\u611f\u3058\u3066\u3044\u308b\u3068\u304d\u306b\u305d\u306e\u76ee\u306b\u3046\u304b\u304c\u3044\u77e5\u308b\u3053\u3068\u306e\u3067\u304d\u308b\u300c\u4f55\u304b\u300d\u3067\u3059\u3002"}
{"translation": "\u4f55\u304b\u61c7\u9858\u3059\u308b\u3088\u3046\u306a\u3001\u5b50\u4f9b\u3089\u3057\u3044\u3001\u9ed9\u3063\u3066\u8010\u3048\u308b\u3088\u3046\u306a\u3082\u306e\u3002"}
{"translation": "\u72e1\u733e\u3067\u3068\u3066\u3082\u8ce2\u3044\u4eba\u3005\u306e\u76ee\u306f\u305d\u3093\u306a\u98a8\u306b\u306f\u306a\u3089\u306a\u3044\u3002"}
{"translation": "\u79c1\u306f\u305f\u3081\u606f\u3092\u3064\u304d\u3001\u30da\u30f3\u3092\u7f6e\u3044\u3066\u3001\u30b3\u30ab\u30fc\u30c9\u306e\u7d33\u58eb\u3092\u5f85\u3061\u59cb\u3081\u307e\u3057\u305f\u3002"}
{"translation": "\u521d\u3081\u3066\u306e\u4f5c\u5bb6\u3084\u7de8\u96c6\u306e\u79d8\u5bc6\u306b\u3064\u3044\u3066\u77e5\u3089\u306a\u3044\u4eba\u3005\u306f\u3001\u300c\u7de8\u96c6\u300d\u3068\u3044\u3046\u8a00\u8449\u3092\u805e\u3044\u305f\u6642\u306b\u795e\u8056\u306a\u6050\u6016\u3092\u611f\u3058\u3001\u81ea\u5206\u3092\u5f85\u305f\u305b\u308b\u6642\u9593\u304c\u591a\u304f\u306a\u308a\u307e\u3059\u3002"}
{"translation": "\u5f7c\u3089\u306f\u3001\u7de8\u96c6\u8005\u306e\u300c\u304a\u9858\u3044\u300d\u306e\u5f8c\u3001\u9577\u3044\u9593\u54b3\u3092\u3057\u3001\u9f3b\u3092\u3059\u3059\u308a\u3001\u3086\u3063\u304f\u308a\u3068\u30c9\u30a2\u3092\u958b\u3051\u3001\u3088\u308a\u3086\u3063\u304f\u308a\u3068\u5165\u5ba4\u3057\u3001\u305d\u308c\u306b\u3088\u3063\u3066\u304b\u306a\u308a\u306e\u6642\u9593\u3092\u53d6\u3089\u308c\u307e\u3059\u3002"}
{"translation": "\u30b3\u30ab\u30fc\u30c9\u3092\u3064\u3051\u305f\u7d33\u58eb\u306f\u81ea\u5206\u3092\u5f85\u305f\u305b\u307e\u305b\u3093\u3067\u3057\u305f\u3002"}
{"translation": "\u30a2\u30f3\u30c9\u30ec\u30a4\u306e\u5f8c\u3067\u30c9\u30a2\u304c\u9589\u307e\u3063\u305f\u3068\u305f\u3093\u3001\u79c1\u306e\u30aa\u30d5\u30a3\u30b9\u3067\u80cc\u304c\u9ad8\u304f\u3066\u80a9\u5e45\u306e\u5e83\u3044\u7537\u304c\u898b\u3048\u307e\u3057\u305f\u3002"}
{"translation": "\u5f7c\u306f\u4e00\u65b9\u306e\u624b\u306b\u7d19\u3067\u5305\u307e\u308c\u305f\u3082\u306e\u3092\u6301\u3061\u3001\u3082\u3046\u4e00\u65b9\u306e\u624b\u306b\u306f\u30b3\u30ab\u30fc\u30eb\u4ed8\u304d\u306e\u8ecd\u5e3d\u3092\u6301\u3063\u3066\u3044\u307e\u3057\u305f\u3002"}
{"translation": "\u79c1\u3068\u306e\u4f1a\u898b\u3092\u5f37\u304f\u6c42\u3081\u3066\u3044\u305f\u305d\u306e\u4eba\u7269\u306f\u3001\u79c1\u306e\u7269\u8a9e\u3067\u975e\u5e38\u306b\u76ee\u7acb\u3064\u5f79\u5272\u3092\u679c\u305f\u3057\u3066\u3044\u307e\u3059\u3002"}
{"translation": "\u5f7c\u306e\u5916\u898b\u3092\u8aac\u660e\u3059\u308b\u5fc5\u8981\u304c\u3042\u308a\u307e\u3059\u3002"}
{"translation": "1880\u5e74\u306e\u3042\u308b4\u6708\u306e\u5348\u5f8c\u3001\u30a2\u30f3\u30c9\u30ec\u30a4\u3068\u3044\u3046\u8b66\u5099\u54e1\u304c\u79c1\u306e\u4e8b\u52d9\u6240\u306b\u5165\u3063\u3066\u304d\u3066\u3001\u306a\u3093\u3068\u306a\u304f\u79d8\u5bc6\u3081\u3044\u305f\u69d8\u5b50\u3067\u5831\u544a\u3057\u307e\u3057\u305f\u3002"}
{"translation": "\u4f55\u8005\u304b\u306e\u7537\u6027\u304c\u7de8\u96c6\u90e8\u306b\u8a2a\u308c\u3001\u7de8\u96c6\u9577\u3068\u306e\u4f1a\u898b\u3092\u5f37\u304f\u6c42\u3081\u3066\u3044\u308b\u3068\u3002"}
{"translation": "\u304a\u305d\u3089\u304f\u5f79\u4eba\u3067\u3057\u3087\u3046\u3001\u3068\u30a2\u30f3\u30c9\u30ec\u30a4\u304c\u4ed8\u3051\u52a0\u3048\u305f\u3001\u895f\u7ae0\u4ed8\u304d\u306e\u3002"}
{"translation": "\u5f7c\u306b\u5225\u306e\u6642\u9593\u306b\u6765\u308b\u3088\u3046\u983c\u3093\u3067\u3001\u79c1\u306f\u8a00\u3063\u305f\u3002"}
{"translation": "\u4eca\u65e5\u306f\u79c1\u306f\u5fd9\u3057\u3044\u3067\u3059\u3002"}
{"translation": "\u7de8\u96c6\u8005\u304c\u571f\u66dc\u65e5\u3060\u3051\u53d7\u3051\u4ed8\u3051\u308b\u3068\u8a00\u3063\u3066\u304f\u3060\u3055\u3044\u3002"}
{"translation": "\u5f7c\u306f3\u65e5\u76ee\u306b\u3082\u6765\u3066\u3001\u3042\u306a\u305f\u306b\u5c0b\u306d\u307e\u3057\u305f\u3002"}
{"translation": "\u5f7c\u306f\u305d\u308c\u304c\u5927\u304d\u306a\u554f\u984c\u3060\u3068\u8a00\u3063\u3066\u3044\u307e\u3059\u3002"}
{"translation": "\u304a\u9858\u3044\u3057\u3066\u3001\u307b\u3068\u3093\u3069\u6ce3\u304d\u305d\u3046\u3067\u3059\u3002"}
{"translation": "\u5f7c\u306f\u571f\u66dc\u65e5\u306b\u306f\u81ea\u7531\u304c\u306a\u3044\u3068\u8a00\u3063\u3066\u3044\u307e\u3059\u3002"}
{"translation": "\u53d7\u3051\u53d6\u308a\u547d\u4ee4\u3092\u51fa\u3057\u307e\u3059\u304b\uff1f"}
