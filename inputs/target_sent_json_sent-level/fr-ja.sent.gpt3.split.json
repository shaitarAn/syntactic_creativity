{"translation": "\u91d1\u66dc\u65e5\u306e\u53e4\u3044\u5fc3\u3001\u5bb6\u306e\u9759\u5bc2\u306e\u4e2d\u306e\u53e4\u3044\u5fc3\u3092\u3002"}
{"translation": "\u4eca\u306f\u307f\u3093\u306a\u51fa\u304b\u3051\u3066\u3057\u307e\u3063\u305f\u3002"}
{"translation": "\u79c1\u3060\u3051\u304c\u6b8b\u3055\u308c\u3066\u3044\u308b\u3002"}
{"translation": "\u9031\u672b\u306b\u6301\u3063\u3066\u3044\u304f\u66f8\u985e\u3092\u6574\u7406\u3057\u306a\u3044\u52c7\u6c17\u304c\u306a\u3044\u304b\u3089\u3001\u75b2\u308c\u679c\u3066\u3066\u3044\u308b\u3002"}
{"translation": "\u6bce\u9031\u91d1\u66dc\u65e5\u306e\u3088\u3046\u306b\u3002"}
{"translation": "\u300c\u305d\u308c\u3058\u3083\u3042\u3001\u3042\u306a\u305f\u306f\u77e5\u3063\u3066\u3044\u308b\u306e\u306d\u3002"}
{"translation": "\u305d\u308c\u3092\u3042\u306a\u305f\u306b\u6301\u3063\u3066\u3044\u304f\u3088\u3046\u306b\u3068\u3001\u5f7c\u304c\u79c1\u306b\u8a00\u3063\u305f\u306e\u3067\u3059\u3002\u300d"}
{"translation": "\u300c\u305d\u308c\u306f\u306a\u3093\u3060\uff1f\u300d"}
{"translation": "\u300c\u3048\u3048\u3068\u3001\u8aad\u66f8\u5668\u304b\u306a\u3001eBook\u304b\u306a\u3001iPad\u304b\u306a\u3001\u308f\u304b\u3089\u306a\u3044\u306a\u3002"}
{"translation": "\u5f7c\u306f\u3042\u306a\u305f\u306e\u30de\u30cb\u30e5\u30b9\u30af\u30ea\u30d7\u30c8\u3092\u5168\u90e8\u5165\u308c\u3066\u3001\u9031\u672b\u307e\u3067\u306b\u6301\u3063\u3066\u3044\u304f\u304b\u3089\u3001\u8377\u7269\u304c\u8efd\u304f\u306a\u308b\u3088\u3068\u8a00\u3063\u3066\u3044\u307e\u3057\u305f\u3002"}
{"translation": "\u8aac\u660e\u3057\u3066\u3082\u3089\u3048\u307e\u3059\u304b\uff1f"}
{"translation": "\u898b\u3066\u304f\u3060\u3055\u3044\u3001\u3042\u306a\u305f\u306e\u30de\u30cb\u30e5\u30b9\u30af\u30ea\u30d7\u30c8\u304c\u5168\u90e8\u8f09\u3063\u305f\u30b9\u30af\u30ea\u30fc\u30f3\u306e\u3088\u3046\u306a\u3082\u306e\u3067\u3059\u3002"}
{"translation": "\u305d\u308c\u3089\u306f\u4eee\u60f3\u306e\u68da\u306e\u4e0a\u306b\u3001\u6728\u88fd\u306e\u4eee\u60f3\u672c\u7269\u306e\u3082\u306e\u304c\u3042\u308a\u307e\u3059\u3002"}
{"translation": "\u305d\u308c\u3089\u3092\u89e6\u308c\u308b\u3068\u3001\u958b\u304d\u307e\u3059\u3002"}
{"translation": "\u305d\u308c\u3089\u304c\u305f\u304f\u3055\u3093\u3042\u308b\u3002"}
{"translation": "\uff12\u65e5\u9593\u3067\u3053\u308c\u3092\u5168\u90e8\u8aad\u3080\u306a\u3093\u3066\u7121\u7406\u3060\u3088\uff01\u898b\u3066\u304f\u3060\u3055\u3044\u3001\u6587\u7ae0\u304c\u958b\u3044\u3066\u3044\u307e\u3059\u3002\u300d"}
